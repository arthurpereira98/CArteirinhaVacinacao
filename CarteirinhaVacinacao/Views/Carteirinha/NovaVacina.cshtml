@using CarteirinhaVacinacao.Models
@model CarteirinhaVacinacao.Models.Vacina
@{
    ViewData["Title"] = "Cadastro Pessoa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Cadastrar novo usuario.</h2>
<div class="row">
    <div class="col-md-5">
        <form method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label for="Nome">Nome </label>
                <input asp-for="Nome" class="form-control" type="text" required name="Nome" id="Nome" />
            </div>
            <div class="form-group">
                <button type="submit" id="salvar" class="btn btn-default btn-info">SALVAR</button>
            </div>
        </form>
    </div>
</div>